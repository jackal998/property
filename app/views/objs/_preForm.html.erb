 <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">
    <% if @obj.id %>
    <h1>Object NO.<%= @obj.id %></h1>
    <% else %>
    <h1>New Object</h1>
    <% end %>
  </div>
  <div class="panel-body">
    <h4 class="pull-left">This is <%= Time.now %> now!</h4>
    <%= link_to "Index" , objs_path , :class => "btn btn-default pull-right" %>
    <br><br>
    <% if @obj.errors.any? %>
      <dl>
      <% @obj.errors.full_messages.each do |msg| %>
      
        <dd> <p class="label label-danger col-md-7"><%= msg %></p></dd>
      
      <% end %>
      </dl>
    <% end %>
  </div>
  <!-- Table -->
  <table class="table table-hover">
		<tr>
			<th></th><th><h3>Informations</h3></th>
		</tr>