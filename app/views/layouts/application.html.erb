<!DOCTYPE html>
<html>
  <head>
    <title>Property</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
	  <div class="container">
	  	<% if flash[:alert] %>
	  		<p class="alert alert-info col-md-1" role="alert"><%= flash[:alert] %></p>
	  	<% end %>
	  	<% if flash[:notice] %>
	  		<p class="alert alert-warning col-md-1" role="alert"><%= flash[:notice] %></p>
	  	<% end %>
	  	<div class="row">
	  		<div class="col-md-12">
	  			<%= render :partial => "navbar" %> 
	    		<%= yield %>

	  		</div>
	  	</div>
	  </div>
  </body>
</html>
